<?xml version="1.0" encoding="utf-8"?><codeeffects xmlns="http://codeeffects.com/schemas/rule/41" xmlns:ui="http://codeeffects.com/schemas/ui/4"><rule id="27358061-0cf6-430f-aab4-6832a236fc34" webrule="4.3.0.2" utc="2016-07-14T21:32:18.8075" type="RuleEngineCodeEffectsSandbox.Models.CreditDeposit.CreditCardDepositModel, RuleEngineCodeEffectsSandbox, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" eval="false"><name>Deposit Transaction Rule</name><definition><if><clause><condition type="equal"><method name="IsPassedMonthlyLimit"><property name="DepositTransaction.Amount" /></method><value type="System.Boolean">true</value></condition></clause><then><set><property name="Notification.Message" /><value>Monthly deposit limit is passed.</value></set><set><property name="IsValid" /><value type="System.Boolean">false</value></set></then><else><if><clause><condition type="equal"><method name="IsNameMatchOnCreditCard"><property name="Customer.CustomerId" /></method><value type="System.Boolean">false</value></condition></clause><then><set><property name="Notification.Message" /><value>Mismatch of name on credit card and customer account.</value></set><set><property name="IsValid" /><value type="System.Boolean">false</value></set></then><else><if><clause><condition type="equal"><method name="IsCreditCardBlocked"><property name="CreditCard.CreditCardNumber" /></method><value type="System.Boolean">true</value></condition></clause><then><set><property name="Notification.Message" /><value>Credit card is blocked.</value></set><set><property name="IsValid" /><value type="System.Boolean">false</value></set></then><else><set><property name="Notification.Message" /><value>Deposit successful.</value></set><set><property name="IsValid" /><value type="System.Boolean">true</value></set></else></if></else></if></else></if></definition><format><lines /></format></rule></codeeffects>